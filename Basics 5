// Getting input via STDIN
const readline = require("readline");

const inp = readline.createInterface({
  input: process.stdin
});

const userInput = [];

inp.on("line", (data) => {
  userInput.push(data);
});

inp.on("close", () => {
  //start-here
  //Your code goes here â€¦ replace the below line with your code logic 
  var n=userInput[0].split(" ");
  //var kabali=n[0];
  //var opponent=n[1];
  console.log(n[1]-n[0]);

  //end-here
});
